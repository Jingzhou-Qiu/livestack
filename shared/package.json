{
  "name": "@livestack/shared",
  "version": "0.0.4",
  "description": "Shared core Livestack logic for both frontend and backend.",
  "scripts": {
    "build": "preconstruct build && cp -r src/graph/wasm dist/",
    "postinstall": "preconstruct dev",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "graphology": "^0.25.4",
    "graphology-types": "^0.24.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.8.4",
    "@types/chai": "^4.3.12",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^16",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "5.4.5"
  },
  "main": "dist/livestack-shared.cjs.js",
  "types": "dist/livestack-shared.cjs.d.ts",
  "module": "dist/livestack-shared.esm.js",
  "type": "module",
  "exports": {
    ".": "./dist/livestack-shared.js",
    "./package.json": "./package.json"
  },
  "preconstruct": {
    "entrypoints": [
      "./index.ts"
    ],
    "exports": true,
    "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
      "typeModule": true,
      "distInRoot": true,
      "importsConditions": true
    }
  },
  "author": ""
}
